%tr
  %td
    = link_to skill_category.name, skill_category_skills_path(skill_category)
  %td
    = link_to edit_skill_category_path(skill_category) do
      %span.glyphicon.glyphicon-edit
    = link_to skill_category, method: :delete, data: { confirm: t('skill_categories.index.delete_confirm') } do
      %span.glyphicon.glyphicon-trash